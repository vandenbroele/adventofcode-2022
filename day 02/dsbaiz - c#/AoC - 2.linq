var handValues = new Dictionary<char, Tuple<char, int>>()
{
	{'X', new Tuple<char, int>('A', 1)},
	{'Y', new Tuple<char, int>('B', 2)},
	{'Z', new Tuple<char, int>('C', 3)},
};

int CalculateScore(char enemyHand, char yourHand)
{
	var handScore = handValues[yourHand].Item2;
	var yourHandEqualized = handValues[yourHand].Item1;
	var roundScore =
		enemyHand == yourHandEqualized ? 3
	 : enemyHand == 'A' && yourHandEqualized == 'B' 
	 	|| enemyHand == 'B' && yourHandEqualized == 'C' 
		|| enemyHand == 'C' && yourHandEqualized == 'A'  ? 6
	 : 0;

	return handScore + roundScore;
}

Console.WriteLine(input.Split("\r\n").Select(x => CalculateScore(x[0], x[2])).Sum());

var roundValues = new Dictionary<char, int>() {
	{'X', 0 },
	{'Y', 3 },
	{'Z', 6 }
};

int GetHandValue(char enemyHand, char roundEnd) =>
	(enemyHand,roundEnd) switch {
		('A', 'X') => 3,
		('A', 'Y') => 1,
		('A', 'Z') => 2,
		('B', 'X') => 1,
		('B', 'Y') => 2,
		('B', 'Z') => 3,
		('C', 'X') => 2,
		('C', 'Y') => 3,
		('C', 'Z') => 1,
		_ => 0
};

int CalculateScore2(char enemyHand, char roundEnd) =>
	roundValues[roundEnd] +  GetHandValue(enemyHand, roundEnd);

Console.WriteLine(input.Split("\r\n").Select(x => CalculateScore2(x[0], x[2])).Sum());
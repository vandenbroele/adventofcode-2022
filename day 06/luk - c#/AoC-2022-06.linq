public int GetResult(string value, int nrOfChars) {
	for (int i = 0; i < value.Length; i++) {
		if(value.Substring(i, nrOfChars).ToCharArray().Distinct().Count() == nrOfChars) return i + nrOfChars;	
	}
	throw new Exception();
	
}


